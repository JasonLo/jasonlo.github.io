{
	"name": "Hugo (Community)",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"VARIANT": "hugo",
			"VERSION": "latest",
			"NODE_VERSION": "14"
		}
	},
	"customizations": {
		"vscode": {
			"settings": {
				"html.format.templating": true
			},
			"extensions": [
				"bungcip.better-toml",
				"davidanson.vscode-markdownlint"
			]
		}
	},
	"forwardPorts": [
		1313
	],
	"remoteUser": "root",
	"features": {
		"ghcr.io/devcontainers/features/github-cli:1": {},
		"ghcr.io/devcontainers/features/python:1": {}
	}
}